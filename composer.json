{
    "name": "alecrabbit/php-accessories",
    "type": "library",
    "description": "PHP Accessories: Classes etc.",
    "keywords": [
        "caller",
        "iterator",
        "memory usage",
        "circular",
        "array",
        "generator",
        "rewindable",
        "pretty",
        "time",
        "percent",
        "bytes",
        "range"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "alecrabbit/php-helpers": "^0.6 || ^0.7 || ^0.8",
        "alecrabbit/php-reports": "^0.3",
        "alecrabbit/php-traits": "^0.3"
    },
    "require-dev": {
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0",
        "symfony/var-dumper": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "AlecRabbit\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlecRabbit\\Tests\\Accessories\\": "tests"
        }
    },
    "minimum-stability": "alpha"
}
